# Create a Resource Group
az group create -n CLIRefRG -l "centralus"

# Create the Virtual Network
az network vnet create --resource-group CLIRefRG -n CLIRefVNET --address-prefixes 10.0.0.0/16 -l "centralus"

#Create the Subnets for the Virtual Network
az network vnet subnet create --resource-group CLIRefRG --vnet-name CLIRefVNET -n Apps --address-prefix 10.0.1.0/24
az network vnet subnet create --resource-group CLIRefRG --vnet-name CLIRefVNET -n Data --address-prefix 10.0.2.0/24
